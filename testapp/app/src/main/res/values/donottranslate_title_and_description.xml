    <string name="standard_android_widgets_test_subtitle">Standard Android Widgets</string>
    <string name="standard_android_widgets_test_description">With TalkBack: swipe between elements, double-tap to activate, use volume keys to operate seekbars.</string>

    <string name="list_test_subtitle">Lists</string>
    <string name="list_test_description"><![CDATA[ With TalkBack: swipe between elements. Accessibility implemented via <a href="https://developer.android.com/reference/android/view/accessibility/AccessibilityNodeInfo.CollectionInfo.html">AccessibilityNodeInfo.CollectionInfo class</a>. ]]></string>

    <string name="grid_test_subtitle">Grids</string>
    <string name="grid_test_description"><![CDATA[ With TalkBack: swipe between elements. Accessibility implemented via <a href="https://developer.android.com/reference/android/view/accessibility/AccessibilityNodeInfo.CollectionInfo.html">AccessibilityNodeInfo.CollectionInfo class</a>. ]]></string>

    <string name="recycler_view_test_subtitle">Scrollable, Flexible List</string>
    <string name="recycler_view_test_description">With TalkBack: swipe between items, listen to feedback.</string>

    <string name="standard_tab_widget_test_subtitle">Standard Tab Widget</string>
    <string name="standard_tab_widget_test_description">With TalkBack: swipe between tabs and content area, double-tap to select a tab, swipe with two fingers to switch between tab pages.</string>

    <string name="radio_group_test_subtitle">Group of Radiobuttons</string>
    <string name="radio_group_test_description">With TalkBack: swipe between radiobuttons, double-tap to select .</string>

    <string name="popup_view_test_subtitle">Popup Menu</string>
    <string name="popup_view_test_description">With TalkBack: swipe to the menu, double-tap to activate, explore menu items.</string>

    <string name="section_header_test_subtitle">Section Headers Inside Collections</string>
    <string name="section_header_test_description"><![CDATA[ With TalkBack: swipe between items, listen to feedback. Accessibility implemented via <a href="https://developer.android.com/reference/android/view/accessibility/AccessibilityNodeInfo.CollectionItemInfo.html#isHeading()">isHeading method</a>. ]]></string>

    <string name="value_picker_test_subtitle">Value Pickers</string>
    <string name="value_picker_test_description">Date Picker, Time Picker &amp; Number Picker. With TalkBack: swipe between items, double-tap to activate, choose OK to complete date or time entry.</string>

    <string name="alert_dialog_test_subtitle">Alert Dialog</string>
    <string name="alert_dialog_test_description">With TalkBack: swipe to the dialog, double-tap to activate, listen to feedback, explore dialog.</string>

    <string name="notification_test_subtitle">Standard and Custom Notifications</string>
    <string name="notification_test_description">With TalkBack: swipe between items, double-tap to generate standard or custom notification, explore status bar for notification text.</string>

    <string name="toast_test_subtitle">Informational Messages</string>
    <string name="toast_test_description">With TalkBack: swipe between buttons, double-tap to generate standard and custom toasts.</string>

    <string name="view_pager_test_subtitle">Multi-page View</string>
    <string name="view_pager_test_description">With TalkBack: swipe to multi-page view, use two fingers to switch pages.</string>

    <string name="web_view_test_subtitle">View with Web Content</string>
    <string name="web_view_test_description">With TalkBack: swipe between web elements, double-tap to activate, swipe up and down to select element type. Note: requires Chrome 50 or above to work.</string>

    <string name="web_view_in_scrollable_view_test_subtitle">WebView Inside ScrollView</string>
    <string name="web_view_in_scrollable_view_test_description">With TalkBack: swipe between web elements until the view scrolls, double-tap to activate. Note: requires Chrome 50 or above to work.</string>

    <string name="one_line_text_field_test_subtitle">One-line Text Field</string>
    <string name="one_line_text_field_test_description">With TalkBack: swipe to field, double-tap to activate, type. Open Local Context Menu (up-then-right) to perform editing operations.</string>

    <string name="multi_line_text_field_test_subtitle">Multi-line Text Area</string>
    <string name="multi_line_text_field_test_description">With TalkBack: swipe to text area, double-tap to activate, type. Use Local Context Menu (up-then-right) to perform editing operations.</string>

    <string name="URL_span_test_subtitle">Links to Other Resources</string>
    <string name="URL_span_test_description">With TalkBack: swipe between items. When you hear a &quot;trill&quot; sound, open Local Context Menu (up-then-right), then pick the &quot;Links&quot; option.</string>

    <string name="clickable_span_test_subtitle">Clickable Targets to Other Resources</string>
    <string name="clickable_span_test_description">This test currently does not do anything with TalkBack.</string>

    <string name="view_grouping_test_subtitle">Groupping Views for Readability</string>
    <string name="view_grouping_test_description">With TalkBack: swipe between items, observe screen layout, listen to spoken feedback.</string>

    <string name="custom_action_test_subtitle">Developer-defined Accessibility Actions</string>
    <string name="custom_action_test_description"><![CDATA[ With TalkBack: swipe to field, listen to feedback, open Local Context Menu (up-then-right), pick a custom action from the menu. Accessibility implemented via <a href="https://developer.android.com/reference/android/view/accessibility/AccessibilityNodeInfo.AccessibilityAction.html">AccessibilityNodeInfo.AccessibilityAction class</a> ]]></string>

    <string name="dismissible_view_test_subtitle">User-dismissible View</string>
    <string name="dismissible_view_test_description">With TalkBack: swipe between items. Upon hearing &quot;dismissible&quot;, open Local Context Menu (swipe up-then-right), then choose &quot;dismiss&quot; from Custom Actions. Also, use buttons to dismiss and restore the view.</string>

    <string name="collapsible_expandable_view_test_subtitle">Expandable and Collapsible Views</string>
    <string name="collapsible_expandable_view_test_description">With TalkBack: swipe between expandable/collapsible items, double-tap to expand or collapse.</string>

    <string name="live_region_test_subtitle">Dynamic Areas with Changing Content</string>
    <string name="live_region_test_description">With TalkBack: swipe between buttons, double-tap to generate different types of live regions, listen to feedback.</string>

    <string name="announce_for_accessibility_test_subtitle">Assertive Screen Reader Announcement</string>
    <string name="announce_for_accessibility_test_description">With TalkBack: swipe to button, double-tap to generate announcement, listen to feedback.</string>

    <string name="TTS_span_test_subtitle">Tell TTS how to Pronounce Things</string>
    <string name="TTS_span_test_description"><![CDATA[ With TalkBack: swipe between items, listen to feedback. See <a href="https://developer.android.com/reference/android/text/style/TtsSpan.html">more TTSSpan types</a>. ]]></string>

    <string name="focus_test_subtitle">Traversal Order</string>
    <string name="focus_test_description">With TalkBack: swipe between items, look at the screen layout and listen to spoken feedback.</string>

    <string name="auto_scroll_test_subtitle">List Auto-scrolling</string>
    <string name="auto_scroll_test_description">With TalkBack: swipe between list items, observe screen.</string>

    <string name="important_for_accessibility_test_subtitle">Expose or Hide Elements for Accessibility</string>
    <string name="important_for_accessibility_test_description">With TalkBack: swipe between elements, observe screen, listen to feedback.</string>

    <string name="content_description_test_subtitle">Content Descriptions</string>
    <string name="content_description_test_description">With TalkBack: swipe between items, observe screen, listen to feedback.</string>

    <string name="label_test_subtitle">Labeling Using On-screen Text</string>
    <string name="label_test_description">With TalkBack: swipe between items, observe screen, listen to feedback.</string>

    <string name="role_description_test_subtitle">Extra Accessibility Information</string>
    <string name="role_description_test_description">With TalkBack: swipe between items, observe screen, listen to feedback.</string>

    <string name="audio_player_test_subtitle">Audio Player</string>
    <string name="audio_player_test_description">With TalkBack: swipe to &quot;Play&quot; button, double-tap to play or pause sound.</string>

    <string name="preference_fragment_test_subtitle">Various Preference Fragments</string>
    <string name="preference_fragment_test_description">With TalkBack: swipe between buttons, double-tap to open fragments, observe spoken feedback</string>

    <string name="edit_field_test_subtitle">Edit Fields with Hint and contentDescription</string>
    <string name="edit_field_test_description">With TalkBack: swipe between fields, observe spoken feedback for each field.</string>

    <string name="password_field_test_subtitle">Password Fields</string>
    <string name="password_field_test_description">With TalkBack: swipe between fields, listen to feedback. Double-tap to edit, listen to feedback.</string>

    <string name="focus_synchronization_test_subtitle">Input and Accessibility Focus Synchronization</string>
    <string name="focus_synchronization_test_description">With TalkBack: swipe between items, double-tap to activate, observe input and accessibility focus.</string>

    <string name="security_overlay_test_subtitle">Security Overlay</string>
    <string name="security_overlay_test_description">With TalkBack: dim the screen via Global Context Menu (down-then-right), swipe to button, double-tap to open security overlay.</string>

</resources>
